                                @Html.ValidationMessageFor(c => c.Entity.SchoolPassBy) 
                            </div> 
                        </div> 
                        <div class="form-group"> 
                            <label class="col-sm-2 control-label">@Html.LabelFor(c => c.Entity.SchoolPassTime)</label> 
