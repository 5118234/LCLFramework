  <add name="EDM" connectionString="Data Source=.;Initial Catalog=EDM;User ID=sa;Password=123456;MultipleActiveResultSets=true;" providerName="System.Data.SqlClient"/>
  </connectionStrings>
  <appSettings>
    <add key="webpages:Enabled" value="false"/>
