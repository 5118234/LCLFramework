                            <div class="col-sm-10"> 
                                @Html.TextBoxFor(c => c.Entity.CountyPassTime, new { @class = "form-control", @placeholder = "请输入县局审核时间" }) 
                                @Html.ValidationMessageFor(c => c.Entity.CountyPassTime) 
                            </div> 
                        </div> 
                        <div class="form-group"> 
                            <label class="col-sm-2 control-label">@Html.LabelFor(c => c.Entity.CountyPassComments)</label> 
                            <div class="col-sm-10"> 
                                @Html.TextBoxFor(c => c.Entity.CountyPassComments, new { @class = "form-control", @placeholder = "请输入县局审核意见" }) 
                                @Html.ValidationMessageFor(c => c.Entity.CountyPassComments) 
                            </div> 
                        </div> 
                        <div class="form-group"> 
                            <label class="col-sm-2 control-label">@Html.LabelFor(c => c.Entity.IsPassCity)</label> 
                            <div class="col-sm-10"> 
                                @Html.TextBoxFor(c => c.Entity.IsPassCity, new { @class = "form-control", @placeholder = "请输入市局审核通过" }) 
                                @Html.ValidationMessageFor(c => c.Entity.IsPassCity) 
                            </div> 
                        </div> 
                        <div class="form-group"> 
                            <label class="col-sm-2 control-label">@Html.LabelFor(c => c.Entity.CitylPassTime)</label> 
                            <div class="col-sm-10"> 
                                @Html.TextBoxFor(c => c.Entity.CitylPassTime, new { @class = "form-control", @placeholder = "请输入市局审核时间" }) 
                                @Html.ValidationMessageFor(c => c.Entity.CitylPassTime) 
                            </div> 
                        </div> 
                        <div class="form-group"> 
                            <label class="col-sm-2 control-label">@Html.LabelFor(c => c.Entity.CityPassBy)</label> 
                            <div class="col-sm-10"> 
                                @Html.TextBoxFor(c => c.Entity.CityPassBy, new { @class = "form-control", @placeholder = "请输入市局审核者" }) 
                                @Html.ValidationMessageFor(c => c.Entity.CityPassBy) 
                            </div> 
                        </div> 
                        <div class="form-group"> 
                            <label class="col-sm-2 control-label">@Html.LabelFor(c => c.Entity.CityPassComments)</label> 
                            <div class="col-sm-10"> 
                                @Html.TextBoxFor(c => c.Entity.CityPassComments, new { @class = "form-control", @placeholder = "请输入市局审核意见" }) 
                                @Html.ValidationMessageFor(c => c.Entity.CityPassComments) 
                            </div> 
                        </div> 
                        <div class="form-group"> 
                            <label class="col-sm-2 control-label">@Html.LabelFor(c => c.Entity.IsPassProvince)</label> 
