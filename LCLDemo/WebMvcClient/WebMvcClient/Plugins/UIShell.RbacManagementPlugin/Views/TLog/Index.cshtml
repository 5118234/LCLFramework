@using LCL.MvcExtensions;
@{
    ViewBag.Title = "系统操作日志";
}
<!-- Page title -->
<div class="page-title">
    <h2><i class="icon-desktop color"></i> 系统操作日志 <small>显示所有系统操作日志</small></h2>
    <hr />
</div>
<!-- Page title -->
<div class="awidget">
    <div class="awidget-head">
        <div class="row">
            <div class="col-md-10">
                <span>总数：@Model.TotalCount</span>
            </div>
        </div>
    </div>
    <div class="awidget-body">
        <div class="row">
            <div class="col-md-12">
                <table class="table table-hover table-bordered">
                    <thead>
                        <tr>
                            <th>单位</th>
                            <th>用户</th>
                            <th>内容</th>
                            <th>更新时间</th>
                        </tr>
                    </thead>
                    <tbody>
                        @{
                            foreach (var view in Model.PagedModels)
                            {
                                <tr>
                                    <td>@view.Org_Id</td>
                                    <td>@view.UserId</td>
                                    <td>@view.Content</td>
                                    <td>@view.UpdateDate</td>
                                </tr>
                            }
                        }
                    </tbody>
                </table>
            </div>
        </div>
        @Html.Partial("_PagedListBottom")
    </div>
</div>





 
