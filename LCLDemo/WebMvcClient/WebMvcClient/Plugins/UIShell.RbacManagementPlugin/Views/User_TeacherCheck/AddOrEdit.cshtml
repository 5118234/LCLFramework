@{ 
    ViewBag.Title = "编辑教师信息审核"; 
} 
 
@using LCL.MvcExtensions 
@using UIShell.RbacPermissionService 
@model AddOrEditViewModel<User_TeacherCheck> 
 
<!-- Page title --> 
<div class="page-title"> 
    <h2><i class="icon-desktop color"></i> 教师信息审核 <small>  
          @{ 
            string actionTag=""; 
            if (Model.Added) { actionTag = "新增一个教师信息审核"; } else { actionTag = "编辑一个教师信息审核"; } 
            }@actionTag 
   </small></h2> <hr /> 
</div> 
<!-- Page title --> 
 
<div class="row"> 
    <!--col-md-6 start--> 
    <div class="col-md-12"> 
        <!--box-info start--> 
        <div class="col-md-12"> 
            <!--box-info start--> 
            <div class="awidget"> 
                <div class="awidget-head"> 
                    <h3>教师信息审核</h3> 
                </div> 
                <div class="awidget-body"> 
                    <!--form-horizontal row-border start--> 
                    @using (Html.BeginForm(Model.Added ? "Add" : "Edit", "User_TeacherCheck", FormMethod.Post, new { @class = "form-horizontal", @role = "form" })) 
                    { 
                        @Html.HiddenFor(c => c.CurrentPageNum) 
                        @Html.HiddenFor(c => c.PageSize) 
                        @Html.HiddenFor(c => c.Entity.ID) 
 
                        <div class="form-group"> 
                            <label class="col-sm-2 control-label">@Html.LabelFor(c => c.Entity.IsPassSchool)</label> 
                            <div class="col-sm-10"> 
                                @Html.TextBoxFor(c => c.Entity.IsPassSchool, new { @class = "form-control", @placeholder = "请输入学校审核通过" }) 
                                @Html.ValidationMessageFor(c => c.Entity.IsPassSchool) 
                            </div> 
                        </div> 
                        <div class="form-group"> 
                            <label class="col-sm-2 control-label">@Html.LabelFor(c => c.Entity.SchoolPassBy)</label> 
