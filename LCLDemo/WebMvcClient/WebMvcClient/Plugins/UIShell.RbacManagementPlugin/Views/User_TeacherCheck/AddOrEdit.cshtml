                                @Html.ValidationMessageFor(c => c.Entity.SchoolPassTime) 
                            </div> 
                        </div> 
                        <div class="form-group"> 
                            <label class="col-sm-2 control-label">@Html.LabelFor(c => c.Entity.SchoolPassComments)</label> 
                            <div class="col-sm-10"> 
                                @Html.TextBoxFor(c => c.Entity.SchoolPassComments, new { @class = "form-control", @placeholder = "请输入学校审核意见" }) 
                                @Html.ValidationMessageFor(c => c.Entity.SchoolPassComments) 
                            </div> 
                        </div> 
                        <div class="form-group"> 
                            <label class="col-sm-2 control-label">@Html.LabelFor(c => c.Entity.IsPassCounty)</label> 
